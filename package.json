{
  "name": "polymer-handlebars",
  "version": "0.1.0",
  "description": "Transform polymer template definitions into handlebars templates",
  "homepage": "https://github.com/TimvdLippe/polymer-handlebars",
  "bugs": "https://github.com/TimvdLippe/polymer-handlebars/issues",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/TimvdLippe/polymer-handlebars.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "LICENSE",
    "lib/",
    "custom_typings/",
    "bin/"
  ],
  "scripts": {
    "clean": "mkdir -p lib/ && rm -rf lib/",
    "build": "gulp build",
    "demo": "tsc && node lib/index.js --config demo/config.json",
    "release": "npm run compile",
    "lint": "gulp lint",
    "test": "npm run clean && npm run build && npm run lint && mocha",
    "quicktest": "export QUICK_TESTS=true; npm run build && mocha",
    "test:watch": "watchy -w src/ -- npm run quicktest --loglevel=silent",
    "format": "find src | grep '\\.js$\\|\\.ts$' | xargs clang-format --style=file -i",
    "postinstall": "cd demo/ && node ../node_modules/bower/bin/bower install",
    "prepublishOnly": "tsc && npm test"
  },
  "devDependencies": {
    "@types/chai": "^3.4.29",
    "@types/chai-as-promised": "^0.0.29",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.28",
    "@types/node": "^6.0.0",
    "@types/parse5": "^2.2.34",
    "bower": "^1.8.0",
    "chai": "^2.2.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.3.0",
    "clang-format": "=1.0.48",
    "depcheck": "^0.6.3",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-mocha": "^4.3.0",
    "gulp-newer": "^1.2.0",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^2.13.4",
    "jsdoc-to-markdown": "^1.0.3",
    "memory-streams": "^0.1.1",
    "merge-stream": "^1.0.0",
    "mocha": "^3.2.0",
    "performance-now": "^2.1.0",
    "run-sequence": "^1.2.0",
    "sinon": "^1.17.4",
    "source-map-support": "^0.4.2",
    "temp": "^0.8.3",
    "tslint": "^4.1.1",
    "typescript-json-schema": "^0.10.0",
    "watchy": "^0.6.6"
  },
  "dependencies": {
    "@types/fs-extra": "^3.0.0",
    "@types/glob": "^5.0.30",
    "dom5": "^2.1.0",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "glob": "^7.1.1",
    "minimist": "^1.2.0",
    "node-fetch": "^1.6.3",
    "parse5": "^2.2.1",
    "polymer-analyzer": "2.0.0-alpha.40",
    "typescript": "^2.2.0"
  },
  "engines": {
    "node": ">=6"
  }
}
